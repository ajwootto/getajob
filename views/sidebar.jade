block sidebar
	#sidebar
		a(href="#")#work Work Projects <br/>
		a(href="#")#side Side Projects <br/>
		a(href="#")#school School <br/>
		a(href="#")#life Life <br />
		a(href="#")#contact Contact <br />
		a(href="#")#resume Resume
script 
	var imageAjax = function (img) {
		$.ajax({
				url: '/info',
				type: 'POST',
				data: {name: $(img).parent().attr('id')},
				success: function(a,b) {
					$("#imageInfo").html(a);
				}
			})
	}
	var bindImages = function() {
		$("img").each(function() {
			var that = this
			$(this).bind('click', function() {
				console.log('clicked')
				if ($(that).hasClass('active')) {
					$(that).removeClass('active');
					$("#clickoff").removeClass('clickActive');
				}
				else {
					$(that).addClass('active');
					$("#clickoff").addClass('clickActive');
					imageAjax(that);
				}
			})
		})
	};
	$('#sidebar').find("a").each(function() {
		$(this).bind('click', function(e) {
			e.preventDefault();
			$.ajax({
				url: '/nav',
				type: 'POST',
				data: {link: $(this).attr('id')},
				success: function(a,b) {
					$("#content").html(a);
					bindImages();
				}
			})
		})
	});
	$("#work").trigger('click');
	
	